{
  "hosting": {
    "public": "dist/code",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],    
    "rewrites": [
      {
        "source": "/api/**",
        "function": "Federation"
      },
      {
        "source": "/**",
        "destination": "index.html"
      }      
    ]
  }
}
